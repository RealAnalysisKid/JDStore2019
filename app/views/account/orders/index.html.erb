<div class="row">
  <div class="col-md-12">
    <h2>全部订单</h2>
    <table class="table table-bordered">
      <thead>
        <th>ID</th>
        <th>生成时间</th>
      </thead>
      <tbody>
        <% @orders.each do |order| %>
        <tr>
          <td><%= link_to(order.id, order_path(order.token)) %></td>
          <td><%= order.created_at.to_s(:long) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
